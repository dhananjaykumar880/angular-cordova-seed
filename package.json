{
  "name": "com.angular-cordova-seed.app",
  "displayName": "My App",
  "version": "1.0.0",
  "description": "An app of angular-cordova",
  "author": "dhananjaykumar880",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "setup": "npm i && cordova prepare && cordova-splash --splash=res/splash.png && cordova-icon --icon=res/icon.png",
    "start": "ng serve --aot",
    "start:proxy": "ng serve --proxy-config proxy.conf.js",
    "build": "ng build",
    "build:dev": "ng build --base-href . && cordova prepare",
    "build:prod": "ng build --prod --base-href . && cordova prepare",
    "run:ios": "npm run build:dev && cordova run ios",
    "run:android": "npm run build:dev && cordova run android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-customurlscheme": "^4.3.0",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-file-transfer": "^1.7.1",
    "cordova-plugin-nano-sqlite": "1.1.1",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-settings-hook": "^0.2.7",
    "cordova-plugin-sfauthenticationsession": "^1.0.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-themeablebrowser": "^0.2.17",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.2.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jshashes": "^1.0.7",
    "material-design-icons": "^3.0.1",
    "nano-sql": "1.2.5",
    "ngx-cookie-service": "^1.0.10",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-statusbar": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-file": {},
      "cordova-plugin-settings-hook": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "myapp",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-plugin-sfauthenticationsession": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-nano-sqlite": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-themeablebrowser": {}
    },
    "platforms": [
      "ios"
    ]
  }
}
